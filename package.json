{
  "name": "api-tester",
  "version": "1.0.0",
  "description": "Command line API Testing Tools",
  "scripts": {
    "build": "webpack",
    "start": "ts-node --files -r tsconfig-paths/register src/index.ts",
    "prepare": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "bin": {
    "api-tester": "./bin/api-tester.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogty/api-tester.git"
  },
  "author": "ogty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ogty/api-tester/issues"
  },
  "homepage": "https://github.com/ogty/api-tester#readme",
  "dependencies": {
    "axios": "^1.2.1",
    "enquirer": "^2.3.6",
    "glob": "^8.0.3",
    "tsconfig-paths": "^4.1.1"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  }
}
